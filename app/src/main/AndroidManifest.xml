<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.graduationproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CarStore">

        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.CarStore.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".OnboardingActivity"
            android:exported="false"
            android:theme="@style/Theme.CarStore.NoActionBar" />

        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:theme="@style/Theme.CarStore.NoActionBar" />

        <activity
            android:name=".CarSelectionActivity"
            android:label="Select Car"
            android:exported="false"
            android:theme="@style/Theme.CarStore.NoActionBar" />

        <activity
            android:name=".CarDetailActivity"
            android:label="Car Details"
            android:exported="false"
            android:theme="@style/Theme.CarStore.NoActionBar" />

        <activity
            android:name=".DocumentInput"
            android:label="Document"
            />
        <activity
            android:name=".DisplayActivity"
            android:label="Confirm Information"
            />

        <activity
            android:name=".Devolopers_Display"
            android:label="Best Team"
            />
    </application>

</manifest>
